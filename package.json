    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "@types/cors": "^2.8.17",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.3",
    "mongoose": "^8.1.1",
    "mysql2": "^3.9.2",
    "reflect-metadata": "^0.2.1",
    "typeorm": "^0.3.20"
  }
